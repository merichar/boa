<% if not organization_booth_chair_list.empty? %>
  <table class="table table-striped">
    <tr>
      <th>Name</th>
      <th>AndrewID</th>
      <th>Phone Number</th>
    </tr>
	<% organization_booth_chair_list.each do |chair| %>
      <tr>
        <td><%= link_to chair.participant.name, chair.participant %></td>
        <td><%= link_to chair.participant.andrewid, chair.participant %></td>
        <td class="no_link"><%= number_to_phone chair.participant.phone_number || "N/A" %></td>
      </tr>
    <% end %>
  </table>
<% else %>
  This organization has no booth chairs.
<% end %>